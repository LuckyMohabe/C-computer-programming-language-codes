#include<stdio.h>

int main()
{
   int number;
   printf("Enter a number");
   scanf("%d",&number);
  
    if(number==1){
    printf("your number is 1 \n");
    }
    else if(number==2){
    printf("your number is 2 \n");
    }
    else if(number==3){
    printf("your number is 3 \n");
    }
    else if(number==4){
    printf("your number is 4 \n");
    }
    else{
    printf("your number is not 1,2,3,4 \n");
    }
    return 0;
}
